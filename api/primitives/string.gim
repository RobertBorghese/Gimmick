# =================================================
# * String
#
# Methods available to the string primitive.
# =================================================

@global
modify string:
	inject get length -> int:
		@if(cpp) @untyped return this.size();
		@elseif(js) @untyped return this.length;

	inject def toString() -> string:
		return this;

	def bla() -> int:
		return 12;
